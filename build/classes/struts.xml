<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="default" extends="struts-default">
		<action name="catalogo"
			class="org.action.catalogo.ActionCatalogo" method="catalogo">
			<result name="success">/formcatalogo.jsp</result>

		</action>
		
		<action name="autentica" class="org.action.cuenta.ActionCuenta" method="autentica">
		<result name="success">/index.jsp</result>
		<result name="error">/formautetica.jsp</result>
		</action>
		
		<action name="alta" class="org.action.hta.ActionHta" method="add">
		<result name="input">/formalta.jsp</result>
		<result name="success">/index.jsp</result>
		</action>
	</package>
</struts>